{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public",
        "installCommand": "npm install && npm --prefix yearguess install",
        "buildCommand": "npm --prefix yearguess run build && mkdir -p public && cp -a yearguess/dist/. public/"
      }
    }
  ],
  "cleanUrls": true,
  "github": {
    "silent": false
  }
}
